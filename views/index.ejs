<html>
<head>
  <title><%= title %></title>
  <link rel="stylesheet" type="text/css" href="style.css"/>
</head>
<body>
  <div class="bg_photos">
    <% tweets.statuses.forEach(function(status) { %>
      <img src="<%= status.user.profile_image_url.replace('_normal', '') %>">
      <!-- <%= status.user.screen_name %>: <%= status.text %><br> -->
    <% }) %>
  </div>
  <div class="overlay">
    <div class="review">
      <p>Test review</p>
    </div>
  </div>
</body>
</html>
